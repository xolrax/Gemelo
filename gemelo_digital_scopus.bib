@ARTICLE{Shi20221364,
	author = {Shi, Yangyang and Deng, Xuesong and Tong, Yuqi and Li, Ruotong and Zhang, Yanfang and Ren, Lijie and Si, Weixin},
	title = {Synergistic Digital Twin and Holographic Augmented-Reality-Guided Percutaneous Puncture of Respiratory Liver Tumor},
